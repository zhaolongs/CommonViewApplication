<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <!--加载圆圈的进度颜色-->
        <attr name="circleColor" format="color"/>
        <attr name="circleStartColor" format="color"/>
        <attr name="circleCenterColor" format="color"/>
        <attr name="circleEndColor" format="color"/>
        <!--加载圆圈的背景颜色-->
        <attr name="circleBackgroundColor" format="color"/>
        <!--加载圆圈的宽度-->
        <attr name="circleWidth" format="dimension"/>
        <!--加载圆圈显示大小 半径-->
        <attr name="circleRadius" format="dimension"/>
        <!--加载圆圈 转动一周所用时间-->
        <attr name="circleProgressDuration" format="integer"/>
        <!--加载圆圈中间点大小-->
        <attr name="circlePointSize" format="dimension"/>
        <!--加载圆圈中间点颜色-->
        <attr name="circlePointColor" format="color"/>

        <!--加载圆圈中间显示文本-->
        <attr name="cicrleProgressWaitText" format="string"/>
        <attr name="cicrleProgressWaitTextColor" format="color"/>
        <attr name="cicrleProgressWaitTextSize" format="dimension"/>

        <!--加载圆圈背景颜色-->
        <attr name="isShowCircleBackground" format="boolean"/>
        <!--是否显示加载等待点-->
        <attr name="isShowCirclePoint" format="boolean"/>


        <!--显示进度文字颜色-->
        <attr name="progressTextColor" format="color"/>
        <!--显示进度文字大小-->
        <attr name="progressTextSize" format="dimension"/>


        <attr name="animationType">
            <flag name="defaul" value="1"/>
            <flag name="roate_x" value="2"/>
            <flag name="roate_y" value="3"/>
            <flag name="roate_x_y" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FindPathView">


        <attr name="typeModel">
            <flag name="defaul" value="1"/>
            <flag name="leve1" value="2"/>
            <flag name="leve2" value="3"/>
            <flag name="leve3_anmation_y_restart" value="4"/>
            <flag name="leve3_anmation_y_reserve" value="5"/>
        </attr>

        <!--加载圆圈的进度颜色-->
        <attr name="findColor" format="color"/>
        <!--加载圆圈的背景颜色-->
        <attr name="findBackgroundColor" format="color"/>
        <!--加载圆圈的半径-->
        <attr name="findCicrleRadius" format="dimension"/>
        <!--加载圆圈的宽度-->
        <attr name="findCicrleWidth" format="dimension"/>

        <!--加载时间的宽度-->
        <attr name="findCicrleDuation" format="integer"/>


        <!--加载圆圈的中心圆点-->
        <attr name="isShowCenterOrigin" format="boolean"/>
        <attr name="findCenterOriginColor" format="color"/>
        <!--显示坐标-->
        <attr name="isShowcoord" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="PathCoreView">


        <!--半径 -->
        <attr name="core_radius" format="dimension"/>
        <!--颜色 -->
        <attr name="core_color" format="color"/>
        <!--展开时间 -->
        <attr name="explor_duration" format="integer"/>
        <!--旋转速度 -->
        <attr name="typeSpeed">
            <flag name="defaul_speed" value="1"/>
            <flag name="low_speed" value="2"/>
            <flag name="middle_speed" value="3"/>
            <flag name="fast_speed" value="4"/>
            <flag name="very_fast_speed" value="5"/>
        </attr>
    </declare-styleable>


    <!--滑动开关-->
    <declare-styleable name="CirCleSwitchButton">
        <!--滑动时间 -->
        <attr name="switchButtonDuaration" format="integer"/>
        <!--滑动 关闭 背景-->
        <attr name="switchButtonCloseColor" format="color"/>
        <!--滑动 打开 背景-->
        <attr name="switchButtonOpenColor" format="color"/>
        <!--滑块颜色-->
        <attr name="switchButtonColor" format="color"/>
        <!--控件的大小-->
        <attr name="switchButtonRadius" format="dimension"/>
        <!--开关打开状态-->
        <attr name="switchButtonStatue">
            <flag name="open" value="2"/>
            <flag name="close" value="1"/>
        </attr>
    </declare-styleable>
</resources>